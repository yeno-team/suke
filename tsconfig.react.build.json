{
	"compilerOptions": {
		"declaration": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"composite": true,
		"target": "es5",
		"module": "esnext",
		"moduleResolution": "node"
	},
	"exclude": ["**/*.test.ts", "**/lib"]
}