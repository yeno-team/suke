{
	"ts-node": {
		"files": true
	},
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "dist",
		"rootDir": "src",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"paths": {
			"safe-eval": ["./src/safe-eval.d.ts"]
		}
	},
	"references": [
        {
			"path": "../requests"
		},
		{
			"path": "../suke-core"
		}
	],
	"include": [
		"./src/**/*.ts"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}