{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "dist",
    "rootDir": "src",
    "composite": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true
  },
  "references": [
    {
      "path": "../suke-core"
    },
    {
      "path": "../suke-server"
    },
    {
      "path": "../suke-util"
    }
  ],
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}